<section id="news" class="news">
    <div class="container" data-aos="fade-up">
      <div class="section-title">
        <h2>News Feed</h2>
      </div>
      <div class="row">
        <div class="col-lg-12 d-flex align-items-stretch">
          <div id="news" class="nn">
            <u><h4 style="text-align: center;">News</h4></u><br>
            <div>
              
              <% if (result.length >0){ var i=1; %>
                <% result.forEach(function(row){ %>
              <a href='<%= row.Link1 %>' target='_blank'>  
              <p>
                <%= i %>)
                <%= row.text %>
              </p>
            </a>

              <% i+=1; })} %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>


